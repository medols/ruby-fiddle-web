<div class="row full-width">
  <div class="large-8 columns collapse">
    <%= render partial: "form" %>
  </div>
  <div class="large-4 columns collapse">
    <div id="riddle_result" >
      <div class="panel">

        <h4>Welcome to Fresh </h4>
        <p>MultiCore experience in the browser.</p>
        <hr/>

        <h4>Tutorial with actors</h4>
        <ul class="three-up no-bullet">
          <li>
            <%=link_to "Hello World", "https://fresh-mc.herokuapp.com/riddles/f4d0a/18" %>
